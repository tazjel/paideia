#page {
    display:block;
    position:relative;
    padding:12px 24px;
    width:80%;
    margin:0 auto;
    -moz-box-shadow:0px 1px 6px rgba(0,0,0,0.3) inset;
    border-right:48px solid #F8F0E5;

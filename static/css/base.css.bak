.npc:after {
  content:"<div></div>";
  position:absolute;
  left:-15px;
  top:50%;
  width:0px;
  height:0px;
  border-bottom:15px solid transparent;  /* left arrow slant */
  border-top:15px solid transparent; /* right arrow slant */
  border-right:15px solid #fff; /* bottom, add background color here */
  font-size:0px;
  line-height:0px;
  background:transparent;
}

